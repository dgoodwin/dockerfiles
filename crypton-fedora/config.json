{
  "database": {
    "type": "postgres",
    "user": "crypton_test_user",
    "password": "crypton_test_user_password",
    "database": "crypton_test",
    "host": "localhost",
    "port": 5432
  },
  "defaultKeySize": 32,
  "id_translator": {
    "key_file": "/dev/urandom"
  },
  "cookieSecretFile": "/dev/urandom",
  "privateKey": "privateKeyExample.pem",
  "certificate": "certificateExample.pem",
  "appPath": ".",
  "redis": {
    "host": "localhost",
    "port": 6379,
    "pass": ""
  },
  "commitStatusCheckDelay": 100,
  "commitStatusCheckLimit": 100,
  "securityHeaders": {
    "csrf": false,
    "csp": {
      "policy": {
        "default-src": "'self'",
        "connect-src": "'self' wss://localhost:1025",
        "script-src": "'self'",
        "img-src": "'self'",
        "style-src": "'self'",
        "font-src": "'self'",
        "object-src": "'self'"
      }
    },
    "xframe": "SAMEORIGIN",
    "hsts": {
      "maxAge": 31536000
    }
  },
  "port": 1025,
  "logLevel": "trace"
}

